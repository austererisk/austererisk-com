---
import {config} from '../../../config';
import RootLayout from '@/lib/layouts/root/RootLayout.astro';
import WebsiteLayout from '@/lib/layouts/website/Layout.astro';
import GridSection from '@/lib/components/GridSection.astro';
import PageHeader from '@/lib/components/PageHeader.astro';
import { courses} from '../../content';
import CallToAction from '@/lib/components/CallToAction.astro';
---
<RootLayout {config}>
<WebsiteLayout {config}>
  <PageHeader title={config.site.collections.courses.title} description={config.site.collections.courses.description} />
  <GridSection objects={courses} />
  <CallToAction {config} />
</WebsiteLayout>
</RootLayout>
